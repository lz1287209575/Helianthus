{
  "dashboard": {
    "id": null,
    "title": "Helianthus Enhanced Metrics Dashboard",
    "tags": ["helianthus", "message-queue", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Batch Processing Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(helianthus_batch_duration_ms_sum[5m]) / rate(helianthus_batch_duration_ms_count[5m])",
            "legendFormat": "{{queue}} - Avg Duration"
          },
          {
            "expr": "helianthus_batch_duration_p95_ms",
            "legendFormat": "{{queue}} - P95 Duration"
          },
          {
            "expr": "helianthus_batch_duration_p99_ms",
            "legendFormat": "{{queue}} - P99 Duration"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (ms)",
            "unit": "ms"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Batch Processing Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(helianthus_batch_count_total[5m])",
            "legendFormat": "{{queue}} - Batches/sec"
          },
          {
            "expr": "rate(helianthus_batch_messages_total[5m])",
            "legendFormat": "{{queue}} - Messages/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Throughput",
            "unit": "ops"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Batch Duration Histogram",
        "type": "heatmap",
        "targets": [
          {
            "expr": "increase(helianthus_batch_duration_ms_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (ms)",
            "unit": "ms"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Zero-Copy Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(helianthus_zero_copy_duration_ms_sum[5m]) / rate(helianthus_zero_copy_duration_ms_count[5m])",
            "legendFormat": "Avg Duration"
          },
          {
            "expr": "helianthus_zero_copy_duration_p95_ms",
            "legendFormat": "P95 Duration"
          },
          {
            "expr": "helianthus_zero_copy_duration_p99_ms",
            "legendFormat": "P99 Duration"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (ms)",
            "unit": "ms"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Zero-Copy Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(helianthus_zero_copy_operations_total[5m])",
            "legendFormat": "Operations/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "unit": "ops"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Zero-Copy Duration Histogram",
        "type": "heatmap",
        "targets": [
          {
            "expr": "increase(helianthus_zero_copy_duration_ms_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "yAxes": [
